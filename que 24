#include <stdio.h>

int main() {
    int num, digit, rev = 0;

    printf("Enter a number: ");
    scanf("%d", &num);

    int temp = num;  

    while(temp > 0) {
        digit = temp % 10;         
        rev = rev * 10 + digit;    
        temp /= 10;                 
    }

    printf("The reverse of %d is: %d\n", num, rev);

    return 0;
}
