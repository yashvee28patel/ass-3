#include <stdio.h>

int main() {
    int num, digit, sum = 0;

    printf("Enter a number: ");
    scanf("%d", &num);

    int temp = num;  

    while(temp > 0) {
        digit = temp % 10;   
        sum += digit;        
        temp /= 10;          
    }

    printf("The sum of digits of %d is: %d\n", num, sum);

    return 0;
}
