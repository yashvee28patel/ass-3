#include <stdio.h>
int main() { 
int i, num;  
int largest, smallest;    printf("Enter 100 numbers:\n");   

largest = smallest = num;    

for(i = 2; i <= 100; i++) {    
scanf("%d", &num);    
if(num > largest) {     
largest = num;        }    
if(num < smallest) {       
smallest = num;  
}  
}  
printf("\nThe largest number is: %d\n", largest);   
printf("The smallest number is: %d\n", smallest);    return 0;
}
